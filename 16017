import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int[] arr = new int[4];
		for(int i=0; i<arr.length; i++) {
			arr[i] = sc.nextInt();
		}
		
		if(arr[0]>7 && arr[1]==arr[2] && arr[3]>7) {
			System.out.println("ignore");
		} else {
			System.out.println("answer");
		}
	} 
}
