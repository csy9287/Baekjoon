import java.util.Scanner;

public class Main {
	public static void main(String[] args) throws Exception {
		
		Scanner sc = new Scanner(System.in);
		
		String mbti = sc.nextLine();
		int N = sc.nextInt();
		int count = 0;
		
		for(int i=0; i<=N; i++) {
			count+=mbti.equals(sc.nextLine())?1:0;
		}
		System.out.println(count);
	} 
}
